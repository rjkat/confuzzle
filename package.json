{
  "name": "confuzzle",
  "version": "0.1.0",
  "description": "multiplayer crosswords",
  "engines": {
    "node": "13.7.x"
  },
  "main": "server/index.js",
  "scripts": {
    "test": "jest",
    "preinstall": "npx npm-force-resolutions",
    "build": "parcel build client/index.html client/syntax.html --no-content-hash --no-cache",
    "postbuild": "workbox injectManifest && workbox copyLibraries dist",
    "develop": "NODE_ENV=development nodemon server/index.js",
    "start": "node server/index.js"
  },
  "nodemonConfig": {
    "watch": "server/*,client/*,node_modules/keen-ui/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjkat/confuzzle.git"
  },
  "author": "Rowan Katekar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjkat/confuzzle/issues"
  },
  "homepage": "https://github.com/rjkat/confuzzle#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@confuzzle/ecoji-buffers": "file:@confuzzle/ecoji-buffers",
    "@confuzzle/puz-crossword": "file:@confuzzle/puz-crossword",
    "@confuzzle/puz-sharing": "file:@confuzzle/puz-sharing",
    "@popperjs/core": "^2.5.4",
    "@thi.ng/range-coder": "^1.0.64",
    "base64url": "^3.0.1",
    "blissfuljs": "^1.0.6",
    "compression": "^1.7.4",
    "emojisplosion": "^2.5.0",
    "enolib": "^0.8.2",
    "enotype": "^0.3.0",
    "express": "^4.17.1",
    "express-force-https": "^1.0.0",
    "express-robots-txt": "^0.4.1",
    "https": "^1.0.0",
    "human-readable-ids": "^1.0.4",
    "iconv-lite": "^0.5.2",
    "keen-ui": "^1.3.1",
    "keycode-js": "^2.0.3",
    "lzutf8": "^0.5.6",
    "parcel-bundler": "^1.12.4",
    "prismjs": "^1.22.0",
    "sanitize-html": "^1.27.5",
    "sass": "^1.30.0",
    "sassybitwise": "^1.1.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "uniqid": "^5.2.0",
    "vue": "^2.6.12",
    "vue-responsive": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-jest": "^26.6.3",
    "babel-polyfill": "^6.26.0",
    "babel-types": "^6.26.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "typescript": "^3.9.7",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.12",
    "workbox-cli": "^6.0.2"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}
